{
  "name": "map-places",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development DEBUG=mp* npm-run-all --parallel dev-web dev-admin",
    "dev-web": "cross-env NODE_ENV=development DEBUG=mp* MONGODB_DEBUG=1 babel-watch -D --delay 5 -w ./src/common -w ./src/server ./src/server/server.js",
    "dev-admin": "cd admin && cross-env PORT=8060 node scripts/start.js",
    "build": "cross-env NODE_ENV=production npm-run-all build-web build-admin",
    "build-web": "cross-env NODE_ENV=production next build",
    "build-admin": "cd admin && node scripts/build.js",
    "start": "cross-env NODE_ENV=production npm-run-all --parallel start-web start-admin",
    "start-web": "cross-env NODE_ENV=production forever start --uid map-places --append ./bin/run-server.js",
    "start-admin": "cross-env NODE_ENV=production forever start --uid map-places-admin --append -c 'serve -s -p 8060' ./admin/build",
    "lint-js": "eslint -c ./.eslintrc.js ./bin ./pages ./src",
    "analyze": "cross-env ANALYZE=1 next build",
    "test": "jest --config ./jest.config.js  --coverage",
    "test:watch": "jest --config ./jest.config.js --watch",
    "console": "cross-env DEBUG=mp* babel-node ./bin/run-console.js"
  },
